    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.1",
    "eslint-config-clockwork": "0.0.8",
    "extract-text-webpack-plugin": "^1.0.1",
    "function-bind": "^1.1.0",
    "gulp": "^3.9.1",
